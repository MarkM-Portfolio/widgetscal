<sqlMap namespace="MultiTenancy_MT">
	<update id="updateOrgId" parameterClass="map" >
		UPDATE CALENDAR.$TABLE$ SET ORG_ID = #NEW_ORG_ID# WHERE CALENDAR_UUID = #CALENDAR_UUID#
	</update>
</sqlMap>
