<config id="calendar"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:noNamespaceSchemaLocation="calendar-config.xsd">
		
		<!-- REPEATING EVENT -->
		<repeatingEvent>
			<maxRepeatingCount>700</maxRepeatingCount>
		</repeatingEvent>
		
		<!-- START CALENDAR ICAL FEED CONFIG -->
		<icalFeed>
			<startFrom>-6</startFrom>
			<endTo>12</endTo>
		</icalFeed>
		
		<allowCommentsFromNonCommunityMember>true</allowCommentsFromNonCommunityMember>
		
</config>
		
