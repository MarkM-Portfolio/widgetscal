<sqlMap namespace="SchemaVersion_MT">
	<resultMap id="schemaVersionMap" class="java.util.HashMap">
		<result property="DBSCHEMAVER" javaType="string" column="DBSCHEMAVER" />
		<result property="RELEASEVER" javaType="string" column="RELEASEVER" />
	</resultMap>
	
	<select id="getSchemaVersionForCalendarDB" resultMap="schemaVersionMap">
		SELECT DBSCHEMAVER, RELEASEVER
		FROM CALENDAR.CA_SCHEMA
		WHERE COMPKEY = 'CALENDAR'
	</select>
</sqlMap>